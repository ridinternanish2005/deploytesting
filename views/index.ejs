    <script>
        // Enhanced JavaScript for interactivity
        document.addEventListener('DOMContentLoaded', async function() {
            // Check authentication and get user data from server
            try {
                const response = await fetch('/api/auth/check');
                const authData = await response.json();
                
                if (!authData.authenticated) {
                    // Redirect to login if not authenticated
                    window.location.href = '/';
                    return;
                }

                // Get detailed user data
                const userResponse = await fetch('/api/auth/user');
                const userData = await userResponse.json();

                if (userData.success) {
                    const user = userData.user;
                    
                    // Set user information from server
                    document.getElementById('user-display-name').textContent = user.fullName || user.username;
                    document.getElementById('welcomeUserName').textContent = user.fullName || user.username;
                    
                    // Set file number and session from server data
                    document.getElementById('fileNumberDisplay').textContent = user.fileNo;
                    document.getElementById('sessionDisplay').textContent = user.session;
                    
                    // Store in localStorage for backward compatibility
                    localStorage.setItem('userType', user.userType);
                    localStorage.setItem('session', user.session);
                    localStorage.setItem('username', user.username);
                    localStorage.setItem('fullName', user.fullName);
                    localStorage.setItem('fileNo', user.fileNo);

                    console.log('User data loaded:', user);
                } else {
                    throw new Error('Failed to get user data');
                }
            } catch (error) {
                console.error('Authentication check failed:', error);
                // Fallback to localStorage if available
                const userType = localStorage.getItem('userType') || 'student';
                const session = localStorage.getItem('session') || '2023-24';
                const username = localStorage.getItem('username') || 'Guest User';
                const fullName = localStorage.getItem('fullName') || username;
                const fileNo = localStorage.getItem('fileNo') || username;

                document.getElementById('user-display-name').textContent = fullName;
                document.getElementById('welcomeUserName').textContent = fullName;
                document.getElementById('fileNumberDisplay').textContent = fileNo;
                document.getElementById('sessionDisplay').textContent = session;
            }

            // Menu item click handling
            const menuItems = document.querySelectorAll('.menu li');
            menuItems.forEach(item => {
                item.addEventListener('click', function() {
                    menuItems.forEach(i => i.classList.remove('active'));
                    this.classList.add('active');
                    
                    // Check menu items for redirection
                    const menuText = this.querySelector('a').textContent.trim();
                    
                    // Define page mappings
                    const pageMappings = {
                        "Student Dashboard": "Student Dashboard.html",
                        "Student Due":"Studentdues.html",
                        "Bus-Hosted Opt":"Bus-Hosted Opt.html",
                        "Student Profile":"Student Profile.html",
                        "Question Paper":"Questionpaper.html",
                        "Test Result": "Test Result.html",
                        "Book Search": "Book Search.html",
                        "Soft Skills": "Soft Skills.html",
                        "Technical Stuff": "Technical Stuff.html",
                        "Change Password": "Change Password.html"
                        
                    };
                    
                    // Redirect if the menu item has a mapping
                    if (pageMappings[menuText]) {
                        window.location.href = pageMappings[menuText];
                        return;
                    }
                    
                    // For other menu items, update the header title
                    const welcomeUserName = document.getElementById('welcomeUserName').textContent;
                    document.querySelector('.header h1').textContent = menuText + ' - Welcome, ' + welcomeUserName;
                    console.log(`Loading ${menuText} content...`);
                });
            });

            // Logout functionality
            document.getElementById('logoutBtn').addEventListener('click', function(e) {
                e.preventDefault();
                fetch('/api/auth/logout', { method: 'POST' })
                    .then(response => response.json())
                    .then(data => {
                        if (data.success) {
                            window.location.href = data.redirectUrl || '/';
                        }
                    })
                    .catch(error => {
                        console.error('Logout error:', error);
                        window.location.href = '/';
                    });
            });
            
            // Sidebar toggle
            const toggleBtn = document.querySelector('.toggle-btn');
            const sidebar = document.querySelector('.sidebar');
            const header = document.querySelector('.header');
            const mainContent = document.querySelector('.main-content');

            toggleBtn.addEventListener('click', () => {
                sidebar.classList.toggle('closed');
                
                // Adjust main content and header when sidebar is toggled
                if (sidebar.classList.contains('closed')) {
                    mainContent.style.marginLeft = '0';
                    mainContent.style.width = '100%';
                    header.style.left = '0';
                } else {
                    mainContent.style.marginLeft = '250px';
                    mainContent.style.width = 'calc(100% - 250px)';
                    header.style.left = '250px';
                }
            });
            
            // Add hover effect to cards
            const cards = document.querySelectorAll('.card');
            cards.forEach(card => {
                card.addEventListener('mouseenter', function() {
                    this.style.boxShadow = '0 8px 25px rgba(0, 0, 0, 0.1)';
                });
                
                card.addEventListener('mouseleave', function() {
                    this.style.boxShadow = '0 5px 15px rgba(0, 0, 0, 0.05)';
                });
            });
            
            // Edit Profile Functionality
            const editProfileBtn = document.getElementById('editProfileBtn');
            const editProfileForm = document.getElementById('editProfileForm');
            const saveProfileBtn = document.getElementById('saveProfileBtn');
            const cancelProfileBtn = document.getElementById('cancelProfileBtn');
            const editFileNumber = document.getElementById('editFileNumber');
            const editSession = document.getElementById('editSession');
            
            editProfileBtn.addEventListener('click', () => {
                editProfileForm.style.display = 'block';
                editFileNumber.value = document.getElementById('fileNumberDisplay').textContent;
                editSession.value = document.getElementById('sessionDisplay').textContent;
            });
            
            saveProfileBtn.addEventListener('click', async () => {
                const newFileNumber = editFileNumber.value.trim();
                const newSession = editSession.value.trim();
                
                if (newFileNumber && newSession) {
                    try {
                        // Update on server
                        const response = await fetch('/api/user/profile', {
                            method: 'PUT',
                            headers: {
                                'Content-Type': 'application/json',
                            },
                            body: JSON.stringify({
                                fileNo: newFileNumber,
                                session: newSession
                            })
                        });
                        
                        const result = await response.json();
                        
                        if (result.success) {
                            document.getElementById('fileNumberDisplay').textContent = newFileNumber;
                            document.getElementById('sessionDisplay').textContent = newSession;
                            
                            // Update localStorage
                            localStorage.setItem('fileNo', newFileNumber);
                            localStorage.setItem('session', newSession);
                            
                            editProfileForm.style.display = 'none';
                            alert('Profile updated successfully!');
                        } else {
                            alert('Failed to update profile: ' + result.message);
                        }
                    } catch (error) {
                        console.error('Update profile error:', error);
                        alert('Error updating profile. Please try again.');
                    }
                } else {
                    alert('Please fill in all fields');
                }
            });
            
            cancelProfileBtn.addEventListener('click', () => {
                editProfileForm.style.display = 'none';
            });
            
            // Edit Attendance Table Functionality
            const editAttendanceBtn = document.getElementById('editAttendanceBtn');
            const attendanceTable = document.getElementById('attendanceTable');
            const saveAttendanceBtn = document.getElementById('saveAttendanceBtn');
            const cancelAttendanceBtn = document.getElementById('cancelAttendanceBtn');
            const attendanceActionButtons = document.getElementById('attendanceActionButtons');
            
            let originalAttendanceData = [];
            
            editAttendanceBtn.addEventListener('click', () => {
                // Store original data
                originalAttendanceData = [];
                const rows = attendanceTable.querySelectorAll('tbody tr');
                rows.forEach(row => {
                    const cells = row.querySelectorAll('td');
                    const rowData = Array.from(cells).map(cell => cell.textContent);
                    originalAttendanceData.push(rowData);
                });
                
                // Make table editable
                const editableCells = attendanceTable.querySelectorAll('tbody td:not(:first-child):not(:last-child)');
                editableCells.forEach(cell => {
                    cell.setAttribute('contenteditable', 'true');
                });
                
                // Show action buttons
                attendanceActionButtons.style.display = 'flex';
                editAttendanceBtn.style.display = 'none';
            });
            
            saveAttendanceBtn.addEventListener('click', () => {
                // Calculate percentages and update status
                const rows = attendanceTable.querySelectorAll('tbody tr');
                rows.forEach(row => {
                    const cells = row.querySelectorAll('td');
                    const totalClasses = parseInt(cells[1].textContent);
                    const present = parseInt(cells[2].textContent);
                    
                    if (!isNaN(totalClasses) && !isNaN(present)) {
                        const percentage = (present / totalClasses * 100).toFixed(1);
                        cells[4].textContent = percentage + '%';
                        
                        // Update status
                        if (percentage >= 75) {
                            cells[5].textContent = 'Good';
                            cells[5].className = 'status-high';
                            cells[4].className = 'attendance-percentage attendance-high';
                        } else if (percentage >= 60) {
                            cells[5].textContent = 'Average';
                            cells[5].className = 'status-medium';
                            cells[4].className = 'attendance-percentage attendance-medium';
                        } else {
                            cells[5].textContent = 'Low';
                            cells[5].className = 'status-low';
                            cells[4].className = 'attendance-percentage attendance-low';
                        }
                    }
                });
                
                // Make table non-editable
                const editableCells = attendanceTable.querySelectorAll('tbody td');
                editableCells.forEach(cell => {
                    cell.removeAttribute('contenteditable');
                });
                
                // Hide action buttons
                attendanceActionButtons.style.display = 'none';
                editAttendanceBtn.style.display = 'inline-block';
                
                alert('Attendance data updated successfully!');
            });
            
            cancelAttendanceBtn.addEventListener('click', () => {
                // Restore original data
                const rows = attendanceTable.querySelectorAll('tbody tr');
                rows.forEach((row, rowIndex) => {
                    const cells = row.querySelectorAll('td');
                    cells.forEach((cell, cellIndex) => {
                        cell.textContent = originalAttendanceData[rowIndex][cellIndex];
                    });
                });
                
                // Make table non-editable
                const editableCells = attendanceTable.querySelectorAll('tbody td');
                editableCells.forEach(cell => {
                    cell.removeAttribute('contenteditable');
                });
                
                // Hide action buttons
                attendanceActionButtons.style.display = 'none';
                editAttendanceBtn.style.display = 'inline-block';
            });
            
            // Edit Transactions Table Functionality
            const editTransactionsBtn = document.getElementById('editTransactionsBtn');
            const transactionsTable = document.getElementById('transactionsTable');
            const saveTransactionsBtn = document.getElementById('saveTransactionsBtn');
